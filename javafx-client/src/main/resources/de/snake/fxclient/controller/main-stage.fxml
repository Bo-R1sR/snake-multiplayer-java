<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:id="dialog"
      xmlns="http://javafx.com/javafx"
      fx:controller="de.snake.fxclient.controller.MainController">

    <HBox>
        <Label>MAIN</Label>
    </HBox>

    <Button onAction="#showHistory" text="show History" styleClass="button-raised"></Button>
    <Button onAction="#showGame" text="go to Game" styleClass="button-raised"></Button>
    <Button onAction="#logout" text="logout" styleClass="button-raised"></Button>


</VBox>
